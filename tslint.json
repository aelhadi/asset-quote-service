{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": [
      false
    ],
    "max-classes-per-file": false,
    "max-line-length": [
      true,
      140
    ],
    "member-access": [
      true,
      "no-public"
    ],
    "no-console": [
      false
    ],
    "object-literal-sort-keys": false,
    "quotemark": [
      true,
      "single"
    ],
    "variable-name": [
      true,
      "allow-leading-underscore"
    ]
  },
  "rulesDirectory": []
}